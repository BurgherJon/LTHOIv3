    </env-variables>
    -->

    <env-variables>
        <env-var name="ENDPOINTS_SERVICE_NAME" value="playerapi.endpoints.leavethehouseoutofit.cloud.goog" />
        <env-var name="ENDPOINTS_SERVICE_VERSION" value="2017-08-28r0" />
    </env-variables>


    <use-google-connector-j>true</use-google-connector-j>

</appengine-web-app>